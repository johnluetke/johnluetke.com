{
  "name": "johnluetke.com",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build:508Conference": "lessc src/508Conference/style.less src/508Conference/style.css",
    "build:johnluetke.com": "ng build --prod",
    "build": "run-s build:508Conference build:johnluetke.com docker:stage docker:build",
    "clean": "rm -rf dist/",
    "docker:build": "docker build dist/ -t johnluetke/website:latest -t johnluetke/website:`date +'%Y-%m-%d'`",
    "docker:push": "docker push johnluetke/website:latest && docker push johnluetke/website:`date +'%Y-%m-%d'`",
    "docker:stage": "cp Dockerfile dist/ && cp johnluetke.nginx.conf dist/",
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.8",
    "@angular/common": "^5.2.8",
    "@angular/compiler": "^5.2.8",
    "@angular/core": "^5.2.8",
    "@angular/forms": "^5.2.8",
    "@angular/http": "^5.2.8",
    "@angular/platform-browser": "^5.2.8",
    "@angular/platform-browser-dynamic": "^5.2.8",
    "@angular/router": "^5.2.8",
    "bootstrap": "3.3.5",
    "core-js": "^2.5.3",
    "hammerjs": "^2.0.8",
    "ngx-image-gallery": "^1.3.0",
    "rxjs": "^5.5.6",
    "zone.js": "^0.8.20"
  },
  "devDependencies": {
    "@angular/cli": "1.5.5",
    "@angular/compiler-cli": "^5.2.8",
    "@angular/language-service": "^5.2.8",
    "@types/jasmine": "~2.5.53",
